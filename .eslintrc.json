{
    "env": {
        "browser": true,
        "es2022": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": [2, "always"],
        "space-before-function-paren": [2, "never"]
    }
}
